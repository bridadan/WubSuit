#include <inttypes.h>
#include <stdio.h>

void MIDIParser_init() {
	midiParser.channel = 0;
}

void MIDIParser_parse(uint8_t message[], uint8_t length) {
	if (message[0] == 0x80) {
		Suit_turnOffLightChannel(Suit_MIDIToLightChannel(message[1]));
	} else if (message[0] == 0x90) {
		Suit_turnOnLightChannel(Suit_MIDIToLightChannel(message[1]));
	}
}

